.bottom_left{
  display: flex;
  flex-direction: row;
  width: 28%;
  background: #ecebeb;




}
.left_text>view{
  display: flex;
  width: 100%;
  height: 30px;
  line-height: 40px;
  padding:5px 8px;
  font-size: 13px;
  color: rgb(102, 102, 99);
}

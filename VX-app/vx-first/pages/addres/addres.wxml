<!--pages/addres/addres.wxml-->
<view class="container">
  <view class="top">
    <view class="addres_list" wx:for="{{obj}}" wx:key="index" >
      <view class="addres">
        <view class="addres_left" bindtap="addres" data-index="{{index}}">
          <text>{{item.addres}}</text>
          <text>{{item.name}}</text>
          <text>{{item.gender}}</text>
          <text>{{item.phone}}</text>
        </view>
        <view class="addres_right">
          <image src="/static/image/addres/xiugai.png"></image>
        </view>
      </view>
    </view>
  </view>
  
  <view class="bottom">
    <view class="addAddres_btn">
      <text class="addAddres">新增地址</text>
    </view>
  </view>
</view>

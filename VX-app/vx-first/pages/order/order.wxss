/* pages/order/order.wxss */
.container{
  display: flex;
  padding-top: 90px;
}
.content{
  display: flex;
  width: 100%;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.content_one>image{
  width: 100px;
}
.content_two{
  display: flex;
  flex-direction: column;
  align-items: center;
  color: gray;
  font-size: 13px;
  padding: 30px;
}
.content_three{
  margin-bottom: 20px;
}
.content_three>view{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 190px;
  height: 45px;
  color: white;
  background: rgb(173, 183, 56);
  border-radius: 50px;
}
.content_four{
  font-size: 13px;
  color: rgb(173, 183, 56);
}
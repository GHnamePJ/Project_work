<!--pages/teabar/teabar.wxml-->
<view class="container">
  <view class="content">
    <view class="content_container">
      <!-- one -->
      <view class="content_one">
        <view class="one1">
          <text>0</text><text>/</text><text>3</text>
        </view>
        <view class="one2">
          <progress class="jidutiao" percent="33.33" stroke-width="30" color="#fedc5f" border-radius="15" backgroundColor="#feebd5"></progress>
        </view>
        <view class="one3">
          <text>每3位新用户点亮，得买茶送包券1张</text>

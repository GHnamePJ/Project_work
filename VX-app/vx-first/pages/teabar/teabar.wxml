        </view>
        <view class="one4">
          <image class="one4_image" src="/static/image/teabar/yellowBackground.png" mode="widthFix"></image>
          <text class="one4_text">立即找好友助力 ></text>
        </view>
      </view>
      <!-- two -->
      <view class="content_two">
        <image class="two_image" src="/static/image/teabar/myjiangli.png" mode="widthFix"></image>
        <view class="two_left">
          <text>0</text>
          <tetx class="left_text2">已成功邀请(位)</tetx>
        </view>
        <view class="two_right">
          <text>0</text>
          <text class="left_text2">已获得奖励(次)</text>
        </view>
      </view>
      <!-- three -->
      <view class="content_three">
        <image src="/static/image/teabar/hyjiangli.png" mode="widthFix"></image>
      </view>
    </view>

  </view>
</view>
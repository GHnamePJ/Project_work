<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mybatis.dynamic.proxy.mapper.EmployeeMapper">
    <update id="updateEmployee" parameterType="cn.mybatis.dynamic.proxy.po.Employees">
       update t_employee set empName=#{empName} where id=#{id}
    </update>
</mapper>